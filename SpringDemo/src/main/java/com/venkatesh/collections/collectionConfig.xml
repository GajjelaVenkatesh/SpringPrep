			<list>
				<value>123123123123</value>
				<value>123123123</value>
				<value>123123123123</value>
				<value>123123124124</value>
			</list>
		</property>
		<property name="addresses">
			<set>
				<value>vijayawada</value>
				<value>Hyderabad</value>
				<value>Ongole</value>
				<value>Chennai</value>
