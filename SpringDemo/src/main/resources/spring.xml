<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean class="com.venkatesh.SpringDemo.Student" name="student1">
		<property name="studentId">
			<value>1</value>
		</property>
		<property name="studentName">
			<value>Venkatesh</value>
		</property>
		<property name="studentAddress">
			<value>Nandigama</value>
		</property>
	</bean>
		<bean class="com.venkatesh.SpringDemo.Student" name="student2" p:studentId="2" p:studentName="Sridhar" p:studentAddress="Ongole"/>
	

</beans>
